{
  "name": "espacio-por-el-buen-vivir",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm --workspace apps/web run dev",
    "build": "npm --workspace apps/web run build",
    "start": "npm --workspace apps/web run start",
    "lint": "npm --workspace apps/web run lint",
    "prisma:generate": "npm --workspace @ebv/db run prisma:generate",
    "prisma:migrate": "npm --workspace @ebv/db run prisma:migrate"
  }
}
